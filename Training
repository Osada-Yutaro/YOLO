FROM tensorflow/tensorflow:1.13.1-gpu-py3
RUN apt-get update && apt-get install -y git libsm6 libxrender-dev
RUN pip3 install opencv-python
RUN git clone https://github.com/Osada-Yutaro/YOLO.git
WORKDIR /YOLO
CMD (python3 train.py '../kw_resources/VOCdevkit/VOC2007+2012/' '../kw_resources/model/' 30 1e-3 1) >> ../kw_resources/log.txt
